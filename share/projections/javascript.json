{
  "package.json": {
    "type": "package",
    "alternate": ["package-lock.json", "yarn.lock"]
  },
  "package-lock.json": {
    "alternate": "package.json"
  },
  "yarn.lock": {
    "alternate": "package.json"
  },
  "*.js": {
    "alternate": "{}.test.js"
  },
  "*.test.js": {
    "alternate": "{}.js"
  }
}
