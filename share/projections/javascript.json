{
  "package.json": {
    "type": "package",
    "alternate": ["package-lock.json", "yarn.lock"]
  },
  "package-lock.json": {
    "alternate": "package.json"
  },
  "yarn.lock": {
    "alternate": "package.json"
  },
  "*.test.js": {
    "type": "test",
    "alternate": ["{}.js", "{}.jsx"]
  },
  "*.stories.js": {
    "type": "story",
    "alternate": "{}.jsx"
  },
  "*/index.js": {
    "type": "src",
    "alternate": "{}.test.js"
  },
  "*.js": {
    "type": "src",
    "alternate": "{}.test.js"
  },
  "*.test.ts": {
    "type": "test",
    "alternate": ["{}.ts", "{}.tsx"]
  },
  "*.stories.ts": {
    "type": "story",
    "alternate": "{}.tsx"
  },
  "*/index.ts": {
    "type": "src",
    "alternate": "{}.test.ts"
  },
  "*.ts": {
    "type": "src",
    "alternate": "{}.test.ts"
  },
  "*.tsx": {
    "type": "source",
    "alternate": ["{}.module.scss", "{}.test.tsx", "{}.stories.tsx"]
  },
  "sequelize/migrations/*": {
    "type": "migration"
  },
  "prisma/migrations/*/migration.sql": {
    "type": "migration"
  }
}
