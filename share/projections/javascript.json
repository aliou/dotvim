{
  "package.json": {
    "type": "package",
    "alternate": ["package-lock.json", "yarn.lock"]
  },
  "package-lock.json": {
    "alternate": "package.json"
  },
  "yarn.lock": {
    "alternate": "package.json"
  },
  "*.test.js": {
    "type": "test",
    "alternate": "{}.stories.js"
  },
  "*.stories.js": {
    "type": "story",
    "alternate": "{}.js"
  },
  "*/index.js": {
    "type": "src",
    "alternate": "{}.test.js"
  },
  "*.js": {
    "type": "src",
    "alternate": "{}.test.js"
  }
}
