{
  "_config.yml": {
    "type": "config"
  },
  "_layouts/*.html": {
    "type": "layout"
  },
  "_includes/*.html": {
    "type": "include"
  },
  "_posts/*.md": {
    "type": "post"
  },
  "posts/index.md": {
    "type": "post"
  },
  "posts/index.html": {
    "type": "post"
  },
  "_drafts/*.md": {
    "type": "draft"
  },
  "_assets/*": {
    "type": "asset"
  },
  "Gemfile": {
    "type": "lib",
    "altername": "{}.lock"
  }
}
