{
  "config/routes.rb": {
    "command": "routes"
  },
  "app/serializers/*_serializer.rb": {
    "command": "serializer",
    "affinity": "model",
    "test": "spec/serializers/%s_spec.rb",
    "related": "app/models/%s.rb",
    "template": [
      "class {camelcase|capitalize|colons}Serializer < ActiveModel::Serializer",
      "end"
    ]
  },
  "app/policies/*_policy.rb": {
    "command": "policy",
    "template": [
      "class {camelcase|capitalize|colons}Policy",
      "end"
    ],
    "test": "spec/policies/%s_spec.rb"
  },
  "app/services/*_service.rb": {
    "command": "service",
    "template": [
      "class {camelcase|capitalize|colons}",
      "end"
    ],
    "test": "spec/services/%s_spec.rb"
  },
  "app/queries/*_query.rb": {
    "command": "query",
    "template": [
      "class {camelcase|capitalize|colons}",
      "end"
    ],
    "test": "spec/queries/%s_spec.rb"
  },
  "app/validators/*.rb": {
    "command": "validators",
    "template": [
      "class {camelcase|capitalize|colons}Validator",
      "end"
    ],
    "test": "spec/validators/%s_spec.rb"
  },
  "app/controllers/*_controller.rb": {
    "test": [
      "spec/requests/{}_spec.rb",
      "spec/controllers/{}_controller_spec.rb",
      "test/controllers/{}_controller_test.rb"
    ],
    "alternate": [
      "spec/requests/{}_spec.rb",
      "spec/controllers/{}_controller_spec.rb",
      "test/controllers/{}_controller_test.rb"
    ]
  },
  "spec/factories/*.rb": {
    "command": "factory",
    "affinity": "model",
    "related": "app/models/%s.rb",
    "template": [
      "FactoryBot.define do",
      "  factory :{} do",
      "  end",
      "end"
    ]
  },
  "spec/requests/*_spec.rb": {
    "command": "request",
    "alternate": "app/controllers/{}_controller.rb",
    "template": [
      "require 'rails_helper'",
      "",
      "RSpec.describe '{capitalize}' do",
      "",
      "end"
    ]
  },
  "db/data/*.rb": {
    "command": "data"
  },
  "db/*.yml": {
    "command": "db"
  },
  "db/structure.sql": {
    "command": "structure"
  }
}
